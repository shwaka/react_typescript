{
  "compilerOptions": {
    "sourceMap": true,
    "target": "ES5",
    "module": "ES2015",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "jsx": "react",
    "moduleResolution": "Node",
    "lib": [
      "es2020", "dom"
    ],
    "allowSyntheticDefaultImports": true
  }
}
